{
	"compilerOptions": {
		"noEmit": true,
		"module": "esnext",
		"target": "es2020",
		"lib": ["es2020"],
		"strict": true,
		"alwaysStrict": true,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"types": ["@cloudflare/workers-types", "@types/jest"],
		"skipLibCheck": true
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules/"]
}
